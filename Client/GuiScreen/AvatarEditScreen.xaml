        <RadioButton x:Name="radioButtonAvatar2" Content="" HorizontalAlignment="Left" Margin="186,151,0,0" VerticalAlignment="Top" Checked="radioButtonAvatar2_Checked"/>
        <RadioButton x:Name="radioButtonAvatar1" Content="" HorizontalAlignment="Left" Margin="43,151,0,0" VerticalAlignment="Top" Checked="radioButtonAvatar1_Checked"/>
        <RadioButton x:Name="radioButtonAvatar4" Content="" HorizontalAlignment="Left" Margin="186,283,0,0" VerticalAlignment="Top" Checked="radioButtonAvatar4_Checked"/>
        <RadioButton x:Name="radioButtonAvatar3" Content="" HorizontalAlignment="Left" Margin="43,283,0,0" VerticalAlignment="Top" Checked="radioButtonAvatar3_Checked"/>
